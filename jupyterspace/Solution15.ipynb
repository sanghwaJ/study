   "source": [
    "# Codility - PermMissingElem\n",
    "\n",
    "def solution(A):\n",
    "    # write your code in Python 3.6\n",
    "    A = sorted(A)\n",
    "    \n",
    "    for i in range(len(A)):\n",
    "        if i+1 != A[i]:\n",
    "            return i+1\n",
    "\n",
    "    return len(A) + 1"
   ]
  }
